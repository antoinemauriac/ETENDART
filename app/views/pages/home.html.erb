<div class="pages-home">
  <%# Section avec titre et background avec image opaque avec class bootstrap %>
    <section id="hero-section" class="text-white d-flex align-content-center py-5">
      <div class="container d-flex align-content-center flex-wrap">
        <h1 class="text-center my-3 fs-3">
            Etendart Academy, l'association qui favorise le développement personnel des jeunes grâce à l'initiation au sport et aux arts.
        </h1>
      </div>
    </section>

    <%# section "Notre approche" en parlant des projets et de la philosophie. bg-white avec 6 cards %>

    <section class="bg-white py-5 px-3 rounded-top-5 mt-n4 z-3 position-relative">
      <div class="">
        <h2 id="approche">
          <strong>Notre approche</strong>
        </h2>
        <p>
          Tous nos projets combinent:
        </p>

        <div class="row <!--row-cols-1 row-cols-md-3 row-cols-lg-6--> g-3">

          <div class="col-12 col-sm-6 col-md-4 col-xl-2">
            <%= render "pages/components/card",
            src: "home/card-1.jpg",
            h3: "L'inspiration",
            p: "Par une infrastructure, un parrain, des expériences pédagogiques" %>
          </div>

          <div class="col-12 col-sm-6 col-md-4 col-xl-2">
            <%= render "pages/components/card",
            src: "home/card-2.jpg",
            h3: "Un sport",
            p: "pilier de nos projets comme vecteur de développement et de transmission" %>
          </div>

          <div class="col-12 col-sm-6 col-md-4 col-xl-2">
            <%= render "pages/components/card",
            src: "home/card-3.jpg",
            h3: "Des activités artistiques",
            p: "par une infrastructure, un parrain, des expériences pédagogiques" %>
          </div>

          <div class="col-12 col-sm-6 col-md-4 col-xl-2">
            <%= render "pages/components/card",
            src: "home/card-4.jpg",
            h3: "Un format d'expérience",
            p: "stages, programmes annuels ou événements" %>
          </div>

          <div class="col-12 col-sm-6 col-md-4 col-xl-2">
            <%= render "pages/components/card",
            src: "home/card-5.JPG",
            h3: "Un projet éducatif",
            p: "chaque activité permet de travailler des axes de développement personnel" %>
          </div>

          <div class="col-12 col-sm-6 col-md-4 col-xl-2">
            <%= render "pages/components/card",
            src: "home/card-6.jpg",
            h3: "Des valeurs",
            p: "mixité sociale, parité, fair-play et partage" %>
          </div>

        </div>
      </div>

    </section>

