{
  "background_color": "white",
  "description": "Club Etendart",
  "display": "fullscreen",
  "name": "Club Etendart",
  "icons": [
    <% files = Dir.entries(Rails.root.join("app/assets/images/icons/")).select { |f| !File.directory?(f) && f.match(/.+-(\d{2,3}x\d{2,3}).png/) } %>
    <% files.each_with_index do |file, index| %>
      <% match = file.match(/.+-(?<size>\d{2,3}x\d{2,3})\.png/) %>
      <% if match %>
        {
          "src": "<%= image_path "icons/#{file}" %>",
          "sizes": "<%= match[:size] %>",
          "type": "image/png"
        }<%= "," unless index == files.size - 1 %>
      <% end %>
    <% end %>
  ]
}
